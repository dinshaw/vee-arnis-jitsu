<% @content_heading = "Editing store_category" %>
<p><%= link_to 'Show', @store_category %> | <%= link_to 'Back', store_categories_path %></p>
<%= error_messages_for :store_category %>

<% form_for([:admin,@store_category]) do |f| %>
  <p>
    <b>Title</b><br />
    <%= f.text_field :title %>
  </p>

  <p>
    <b>Description</b><br />
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>


