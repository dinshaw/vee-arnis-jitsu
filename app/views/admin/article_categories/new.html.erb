<% @content_heading = "New article_category" %>
<p><%= link_to 'Back', admin_article_categories_path %></p>
<%= error_messages_for :article_category %>

<% form_for([:admin,@article_category], :html => {:multipart => true}) do |f| %>
  <p>
    <b>Title</b><br />
    <%= f.text_field :title %>
  </p>

  <p>
    <b>Description</b><br />
    <%= f.text_area :description %>
  </p>

<p>
	<b>Photo</b><br />
	<%= f.file_field :uploaded_data_photo %>
</p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>


