<h1>Editing ical_event</h1>

<% form_for(@ical_event) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :stime %><br />
    <%= f.datetime_select :stime %>
  </p>
  <p>
    <%= f.label :etime %><br />
    <%= f.datetime_select :etime %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :body %><br />
    <%= f.text_area :body %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @ical_event %> |
<%= link_to 'Back', ical_events_path %>
