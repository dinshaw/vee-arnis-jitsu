<% @content_heading = "New system" %>
<p><%= link_to 'Back', admin_systems_path %></p>
<%= error_messages_for :system %>

<% form_for([:admin,@system], :html => {:multipart => true}) do |f| %>


<p>
	<b>Title</b><br />
	<%= f.text_field :title %>
</p>

<p>
	<b>Description</b><br />
	<%= f.text_area :description %>
</p>

<p>
	<b>Photo</b><br />
	<%= f.file_field :uploaded_data_photo %>
</p>

<p>
	<%= f.submit "Create" %>
</p>
<% end %>


