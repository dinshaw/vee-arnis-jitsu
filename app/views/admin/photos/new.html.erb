<% @content_heading = "#{@photo_category.title}: New Photo" %>
<p><%= link_to 'Back', admin_photo_category_photos_path(@photo_category) %></p>
<% form_for([:admin, @photo_category, @photo], :html => {:multipart => true}) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :caption %><br />
    <%= f.text_area :caption %>
  </p>
  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>

  <p>
    <b>Image</b><br />
    <%= f.file_field :uploaded_data_photo %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>


